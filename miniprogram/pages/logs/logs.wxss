.sum{
  display:flex;
  flex-wrap:wrap;
  width:750rpx;
  height:350rpx;
  border: 10rpx solid #eeeeee
}
.sum .sum_item{
  width:375rpx;
  height:160rpx;
  text-align: center;
}
.sum .sum_item .sum_item_title{
  height:120rpx;
  line-height:120rpx;
  font-size: 30rpx;
  color: #68747E;
}
.sum .sum_item .sum_item_val{
  height:40rpx;
  line-height:40rpx;
  color:#E7624F;
}
.detail{
  width: 700rpx;
  margin: 0 auto;
}
.detail .detail_title{
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
}
.detail .detail_title .detail_title_text{
  flex: 1;
  font-size: 30rpx;
}
.detail .detail_title .detail_title_type{
  flex: 1;
  text-align: right;
  font-size: 25rpx;
}
.detail .detail_title .detail_title_type text{
  padding-left: 10rpx;
}
.active{
  color:#E7624F;
}
.list_item{
  display: flex;
  height: 40rpx;
  line-height: 50rpx;
}
.list_item .list_item_date{
  widows: 300rpx;
  font-size: 40rpx;
  text-align: right;
}
.list_item .list_item_cate{
  widows: 300rpx;
  font-size: 40rpx;
  text-align: left;
}
.list_item .list_item_time{
  widows: 300rpx;
  font-size: 40rpx;
  text-align: left;
}


